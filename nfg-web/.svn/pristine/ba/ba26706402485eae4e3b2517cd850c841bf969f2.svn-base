                </tr>
                {{/each}}
                </tbody>
                <tfoot>
                <tr>
                    <td colspan="12" style="text-align: center">
                        {{{paginate pagination}}}
                    </td>
                </tr>
                </tfoot>
                {{else}}
                <th class="centralizado">Nenhuma premiação registrada para o sorteio selecionado.</th>
                {{/if}}
            </table>

        </div>
    </script>


