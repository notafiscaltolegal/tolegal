<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org" xmlns:layout="http://www.ultraq.net.nz/thymeleaf/layout" >
<body>
<div layout:fragment="templatesHandlebars">

    <script id="message" type="text/x-handlebars-template">
        <div class="alert alert-dismissible {{type}}" role="alert">
            <button type="button" class="close" data-dismiss="alert">
                <span aria-hidden="true">&times;</span>
                <span class="sr-only">Close</span>
            </button>
            {{message}}
        </div>
    </script>

    <script id="tabelaContribuintes" type="text/x-handlebars-template">
        <div class="row">
            <table class="table table-striped table-hover" style="text-align: left">
                <thead>
                {{#if contribuintes}}
                <tr>
                    <th>Inscrição</th>
                    <th>Razão Social</th>
                    <th>CNPJ</th>
                    <th>Observações</th>
                    <th>Data Início Vigência</th>
                    <th>Ações</th>
                    <th></th>
                    <th></th>
                </tr>
                </thead>
                <tbody>
                {{#each contribuintes}}

                <tr>
                    <td class="maskInscricao">{{numeroInscricao}}</td>
                    <td>{{nomeEmpresa}}</td>
                    <td class="maskCnpj">{{numeroCnpj}}</td>
                    {{#if hasCnaeAutorizado}}
                    {{#if isEmissorEFD}}
                    <td>Cadastrado. (Emitente EFD)</td>
                    <td>&nbsp;</td>
                    <td class="padding-2"><a href="{{../../../urlBase}}/nota/cadastro/{{numeroInscricao}}" class="btn btn-default btn-sm ">Cadastrar Nota</a></td>
                    <td class="padding-2"><a href="{{../../../urlBase}}/mfd/consultar/{{numeroInscricao}}" class="btn btn-default btn-sm ">Consultar MFD</a></td>
           <!--         <td class="padding-2"><a href="{{../../../urlBase}}/ecf/importar/{{numeroInscricao}}" class="btn btn-default btn-sm ">Importação ECF</a></td> -->
                    <td>
                        <a class="iconGlobe">
                            {{#compare qtdReclamacoes 0 operator=">"}}
                            <span class="notification-count" >{{qtdReclamacoes}}</span>
                            {{/compare}}
                        </a>
                        <a href="{{../../../../urlBase}}/reclamacao/consultar/{{numeroCnpj}}" type="button" title="Reclamações"
                           class="btn btn-danger btn-md btn-reclamacao" data-numcnpj="{{numeroCnpj}}"><span  class="glyphicon glyphicon-bullhorn"></span>
                        </a>
                    </td>
                    <td>
                        <a class="iconGlobe">
                            {{#compare qtdMensagens 0 operator=">"}}
                            <span class="notification-count" >{{qtdMensagens}}</span>
                            {{/compare}}
                        </a>
                        <button type="button" class="btn btn-info btn-md btn-mensagem" data-idpessoa="{{idPessoa}}"><span title="Mensagens" class="glyphicon glyphicon-envelope"></span></button>
                    </td>
                    {{else}} {{#if hasCnaeObrigatorio}}
                    <td>Cadastrado. (CNAE Obrigatório)</td>
                    <td>{{dataObrigatoriedadeCnaeFormatada}}</td>
                    <td class="padding-4"><a href="{{../../../../urlBase}}/nota/cadastro/{{numeroInscricao}}" class="btn btn-default btn-sm ">Cadastrar Nota</a></td>
                    <td class="padding-4"><a href="{{../../../../urlBase}}/mfd/consultar/{{numeroInscricao}}" class="btn btn-default btn-sm ">Consultar MFD</a></td>
                    <!-- <td class="padding-2"><a href="{{../../../../urlBase}}/ecf/importar/{{numeroInscricao}}" class="btn btn-default btn-sm ">Importação ECF</a></td> -->
                    <td>
                        <a class="iconGlobe">
                            {{#compare qtdReclamacoes 0 operator=">"}}
                            <span class="notification-count" >{{qtdReclamacoes}}</span>
                            {{/compare}}
                        </a>
                        <a href="{{../../../../urlBase}}/reclamacao/consultar/{{numeroCnpj}}" type="button" title="Reclamações"
                           class="btn btn-danger btn-md btn-reclamacao" data-numcnpj="{{numeroCnpj}}"><span  class="glyphicon glyphicon-bullhorn"></span>
                        </a>
                    </td>
                    <td>
                        <a class="iconGlobe">
                            {{#compare qtdMensagens 0 operator=">"}}
                            <span class="notification-count" >{{qtdMensagens}}</span>
                            {{/compare}}
                        </a>
                        <button type="button"  class="btn btn-info btn-md btn-mensagem" data-idpessoa="{{idPessoa}}"><span  class="glyphicon glyphicon-envelope"></span></button>
                    </td>

                    {{else}} {{#if isEmpresaParticipante}}
                    {{#compare indiResponsavelAdesao 'C' operator='=='}}
                    <td>Contribuinte cadastrado pelo contador.</td>
                    {{else}}
                    <td>Empresa já se cadastrou no NFG.</td>
                    {{/compare}}
                    <td class="col-md-2 padding-4">
                        <div class="input-group col-md-10">
                            <input type="text" class="form-control datepicker" data-pode-alterar="{{podeAlterar}}" data-limite="{{dataLimiteFormatada}}" name="dataInicio" value="{{dataEfetivaParticipacaoFormatada}}" placeholder="Ex: 01/01/2014" required>
                            <span class="input-group-addon glyphicon glyphicon-calendar datepickerIcon"></span>
                        </div>
                    </td>
                    {{#if podeAlterar}}
                    <td class="padding-4"><button type="button" class="btn btn-default btn-sm btn-alterar" data-inscricao="{{numeroInscricao}}">Alterar Cadastro</button></td>
                    {{else}}
                    <td class="padding-4"><a href="{{../../../../../../../urlBase}}/nota/cadastro/{{numeroInscricao}}" class="btn btn-default btn-sm ">Cadastrar Nota</a></td>
                    {{/if}}
                    {{else}}
                    <td>&nbsp;</td>
                    <td class="col-md-2 padding-4">
                        <div class="input-group col-md-10">
                            <input type="text" class="form-control datepicker" data-pode-alterar="{{podeAlterar}}" data-limite="{{dataLimiteFormatada}}" name="dataInicio" placeholder="Ex: 01/01/2014" required >
                            <span class="input-group-addon glyphicon glyphicon-calendar datepickerIcon"></span>
                        </div>
                    </td>
                    <td class="padding-4"><button type="button" class="btn btn-default btn-sm btn-cadastrar" data-inscricao="{{numeroInscricao}}">Cadastrar no NFG</button></td>
                    <td>&nbsp;</td>
                    <td><button type="button" class="btn btn-info btn-md btn-mensagem" data-idpessoa="{{idPessoa}}"><span class="glyphicon glyphicon-envelope"></span></button></td>
                    {{/if}}
                    {{/if}}
                    {{/if}}
                    {{else}}
                    <td>Contribuinte não possui CNAE autorizado para participar do NFG.</td>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                    {{/if}}
                </tr>
                {{/each}}
                </tbody>
                <tfoot>
                <tr>
                    <td colspan="9" style="text-align: center">
                        {{paginate pagination}}
                    </td>
                </tr>
                </tfoot>
                {{else}}
                <thead>
                <tr>
                    <th class="centralizado">Nenhum contribuinte encontrado</th>
                </tr>
                </thead>
                {{/if}}
            </table>
        </div>
    </script>

    <script id="tabelaCFOPs" type="text/x-handlebars-template">
        <div class="row">
            <table class="table table-striped table-hover table-condensed" style="text-align: left">
                <thead>
                {{#if cfopsAutorizados}}
                <tr>
                    <th>Código</th>
                    <th>Descrição</th>
                    <th></th>
                </tr>
                </thead>
                <tbody>
                {{#each cfopsAutorizados}}
                <tr>
                    <td>{{genCfop.codigoCFOP}}</td>
                    <td>{{genCfop.descricaoCFOP}}</td>
                    <td>
                        <button class="btn btn-danger btn-cfop-confirm" data-id-cfop="{{idCfopeAutorizado}}" type="submit">Excluir</button>
                    </td>
                </tr>
                {{/each}}
                </tbody>
                <tfoot>
                <tr>
                    <td colspan="6" style="text-align: center">
                        {{paginate pagination}}
                    </td>
                </tr>
                </tfoot>
                {{else}}
                <thead>
                <tr>
                    <th class="centralizado">Nenhum cfop encontrado</th>
                </tr>
                </thead>
                {{/if}}
            </table>
        </div>
    </script>

    <script id="tabelaEmpresasParticipantes" type="text/x-handlebars-template">
        <div class="row">
            <table class="table table-striped table-hover table-condensed" style="text-align: left">
                <thead>
                {{#if empresas}}
                <tr>
                    <th>Inscrição</th>
                    <th>CNPJ</th>
                    <th>Nome Empresa</th>
                    <th>Nome Fantasia</th>
                    <th>Bairro</th>
                    <th>Município</th>
                </tr>
                </thead>
                <tbody>
                {{#each empresas}}
                <tr>
                    <td class="maskInscricao">{{numeroInscricao}}</td>
                    <td class="maskCnpj">{{numeroCnpj}}</td>
                    <td>{{nomeEmpresa}}</td>
                    <td>{{nomeFantasia}}</td>
                    <td>{{nomeBairro}}</td>
                    <td>{{nomeMunicipio}}</td>
                </tr>
                {{/each}}
                </tbody>
                <tfoot>
                <tr>
                    <td colspan="6" style="text-align: center">
                        {{paginate pagination}}
                    </td>
                </tr>
                </tfoot>
                {{else}}
                <thead>
                <tr>
                    <th class="centralizado">Nenhuma empresa encontrada</th>
                </tr>
                </thead>
                {{/if}}
            </table>
        </div>
    </script>

    <script id="tabelaUltimasNotasInseridas" type="text/x-handlebars-template">
        <div class="row">
            <table class="table table-striped table-hover table-condensed" style="text-align: left">
                <thead>
                {{#if ultimasNotasInseridas}}
                <tr>
                    <th>Número do Documento</th>
                    <th>Tipo de Documento</th>
                    <th>Série</th>
                    <th>Subsérie</th>
                    <th>Data de Emissão</th>
                    <th>CPF Destinatário</th>
                    <th>Valor</th>
                    <th>Editar</th>
                </tr>
                </thead>
                <tbody>
                {{#each ultimasNotasInseridas}}
                <tr>
                    <td class="nrDocList">{{numeroDocumentoFiscal}}</td>
                    <td class="tipoDocList">
                        {{#compare tipoDocumentoFiscal 1 operator="=="}}Modelo 1{{/compare}}
                        {{#compare tipoDocumentoFiscal 2 operator="=="}}Modelo 2{{/compare}}
                        {{#compare tipoDocumentoFiscal 3 operator="=="}}ECF - Antigo{{/compare}}
                    </td>
                    <td class="serieList">
                        {{#compare serieNotaFiscal 1 operator="=="}}D{{/compare}}
                        {{#compare serieNotaFiscal 2 operator="=="}}D - Única{{/compare}}
                        {{#compare serieNotaFiscal 3 operator="=="}}Única{{/compare}}
                    </td>
                    <td class="subSerieList">{{subSerieNotaFiscal}}</td>
                    <td class="dataEmissaoList">{{prettifyDate dataEmissao}}</td>
                    <td >{{maskCpf cpf}}</td>
                    <td >{{maskMoney valorTotal}}</td>
                    <td><a href="#" class="editLink"><img src="/nfg/images/edit-small.png" width="20px;"></a></td>
                </tr>
                {{/each}}
                </tbody>

                <tfoot>
                <tr>
                    <td colspan="6" style="text-align: center">
                        {{paginate pagination}}
                    </td>
                </tr>
                </tfoot>

                {{else}}
                <thead>
                <tr>
                    <th class="centralizado">Nenhuma nota encontrada</th>
                </tr>
                </thead>
                {{/if}}
            </table>
        </div>
    </script>

    <script id="tabelaReclamacaoEmpresaContador" type="text/x-handlebars-template">
        <div class="row">
            <table class="table table-striped table-hover table-condensed" style="text-align: left">
                <thead>
                {{#if listDocumentos}}
                <tr>
                    <th></th>
                    <th>Data Registro</th>
                    <th>Número</th>
                    <th>Reclamante</th>
                    <th>Nota</th>
                    <th>Data Emissão</th>
                    <th>Valor</th>
                    <th style="text-align: center">Andamento</th>
                    <th>Motivo</th>
                    <th>Status</th>
                    <th></th>
                </tr>
                </thead>
                <tbody>
                {{#each listDocumentos}}
                <tr>
                    <td><input type="radio" {{disableRadioBtn}} id="idOptRadio" class="optradio-reclamacao" name="optradio" value="{{id}}" /></td>
                    <td>{{prettifyDate dataReclamacao}}</td>
                    <td>{{id}}</td>
                    <td>{{pessoaParticipante.genPessoaFisica.nome}}</td>
                    <td>{{numero}}</td>
                    <td>{{prettifyDate dataDocumentoFiscal}}</td>
                    <td>{{maskMoneyVirgula valor}}</td>
                    <td style="text-align: center"><a href="#"><img src="/nfg/images/info.png" title="{{listaAndamentoStr}}" class="tooltip-info"width="20px;"></a></td>
                    <td>{{motivoReclamacao.descricao}}</td>
                    <td>{{statusAndamentoStr}}</td>
                    <td>{{#compare tipoExtensao '0' operator=">"}}<a href="{{../../urlDownload}}{{id}}"><img src="/nfg/images/invoice2.png" title="Clique para baixar o anexo" class="tooltip-info"width="20px;"></a>{{/compare}}</td>
                </tr>
                {{/each}}
                </tbody>
                <tfoot>
                <tr>
                    <td colspan="12" style="text-align: center">
                        {{paginate pagination}}
                    </td>
                </tr>
                </tfoot>
                {{else}}
                <th class="centralizado">Nenhuma Reclamação a ser mostrada.</th>
                {{/if}}
            </table>
        </div>
    </script>

    <script id="tabelaMfd" type="text/x-handlebars-template">
        <div class="row">
            <table class="table table-striped table-hover table-condensed" style="text-align: left">
                <thead>
                {{#if listArquivoMemoriaFiscal}}
                <tr>
                    <th>Referência</th>
                    <th>Nome do Arquivo</th>
                    <th>Data Entrega Arquivo</th>
                    <th >Data Início da Apuração</th>
                    <th >Data Fim da Apuração</th>
                    <th style="text-align: center">Status</th>
                    <th>Info Erro</th>
                    <th>CNPJ</th>
                </tr>
                </thead>
                <tbody>
                {{#each listArquivoMemoriaFiscal}}
                <tr>
                    <td>{{mesAnoDate referencia}}</td>
                    <td>{{nome}}</td>
                    <td>{{prettifyDate dataEntrega}}</td>
                    <td >{{prettifyDate dataInicioApuracao}}</td>
                    <td >{{prettifyDate dataFimApuracao}}</td>
                    <td style="text-align: center">
                        {{#compare statusProcessamento 'R' operator="=="}}Recebido{{/compare}}
                        {{#compare statusProcessamento 'A' operator="=="}}Aguardando{{/compare}}
                        {{#compare statusProcessamento 'P' operator="=="}}Em Processamento{{/compare}}
                        {{#compare statusProcessamento 'S' operator="=="}}Processado com Sucesso{{/compare}}
                        {{#compare statusProcessamento 'E' operator="=="}}Processado com Erro{{/compare}}
                        {{#compare statusProcessamento 'C' operator="=="}}Corrigido{{/compare}}
                    </td>
                    <td>{{infoMensagemLog}}</td>
                    <td>{{cnpjEstabelecimento}}</td>
                </tr>
                {{/each}}
                </tbody>
                <tfoot>
                <tr>
                    <td colspan="8" style="text-align: center">
                        {{paginate pagination}}
                    </td>
                </tr>
                </tfoot>
                {{else}}
                <th class="centralizado">Nenhuma informação a ser mostrada.</th>
                {{/if}}
            </table>
        </div>
    </script>

    <script id="tabelaPremiacao" type="text/x-handlebars-template">
        <div class="row">
            <table class="table table-striped table-hover table-condensed" style="text-align: left">
                <thead>
                {{#if listDtoPremiacao}}
                <tr>
                    <th>Sorteio</th>
                    <th>NºBilhete</th>
                    <th>Valor</th>
                    <th>Dados Bancários</th>
                    <th>Dt.Solicitação</th>
                    <th>Dt.Resgate</th>

                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>
                    <th hidden="true"></th>

                    <th></th>
                </tr>
                </thead>
                <tbody>
                {{#each listDtoPremiacao}}
                <td class="sorteioPremio">{{sorteio}}</td>
                <td class="numeroBilhetePremio">{{intSeparadoPorMilhar numeroBilhete}}</td>
                <td class="valorPremio">{{maskMoneyVirgula valor}}</td>


                {{#if tipoResgatePremio}}

                {{#compare tipoResgatePremio 'C' operator="=="}}
                <td > Pagamento com cheque</td>

                {{/compare}}

                {{#compare tipoResgatePremio 'T' operator="=="}}
                {{#if numeroConta}}
                <td >{{nomeBanco}}, Ag. {{codigoAgencia}}, Conta {{numeroConta}}-{{digito}}</td>
                {{else}}
                <td > </td>
                {{/if}}
                {{/compare}}

                {{else}}
                <td > </td>
                {{/if}}


                {{#if dataSolicitacaoResgate}}
                <td >{{prettifyDateIfNotNull dataSolicitacaoResgate}}</td>
                {{else}}
                <td >Não Solicitado</td>
                {{/if}}

                {{#if dataResgate}}
                <td >{{prettifyDateIfNotNull dataResgate}}</td>
                {{else}}
                <td >Não Resgatado</td>
                {{/if}}

                <!--CUIDADO - GAMBIEIRA ABAIXO PARA JOGAR OS VALORES PRO JS-->

                <td class="idPremioBilhete" hidden="true">{{idPremioBilhete}}</td>
                <td class="vlrTributoPremioBilhete" hidden="true">{{valorTributo}}</td>
                <td class="vlrTaxasPremioBilhete" hidden="true">{{valorTaxasBancarias}}</td>
                <td class="tipoResgatePremioBilhete" hidden="true">{{tipoResgatePremio}}</td>
                <td class="dataResgatePremioBilhete" hidden="true">{{prettifyDateIfNotNull dataResgate}}</td>
                <td class="dataLimitePremioBilhete" hidden="true">{{prettifyDateIfNotNull dataLimiteResgate}}</td>
                <td class="dataSolicitacaoPremioBilhete" hidden="true">{{prettifyDateIfNotNull dataSolicitacaoResgate}}</td>
                <td class="valorPremioCalc" hidden="true">{{valor}}</td>
                <td class="vlrTributoParam" hidden="true">{{valorTributoParam}}</td>
                <td class="valorTaxasChequeParam" hidden="true">{{valorTaxasChequeParam}}</td>
                <td class="valorTaxasTransfParam" hidden="true">{{valorTaxasTransfParam}}</td>
                <td class="codgBanco" hidden="true">{{codigoBanco}}</td>
                <td class="codigoAgencia" hidden="true">{{codigoAgencia}}</td>
                <td class="numeroConta" hidden="true">{{numeroConta}}</td>
                <td class="tipoConta" hidden="true">{{tipoConta}}</td>
                <td class="digito" hidden="true">{{digito}}</td>

                {{#if dataResgate}}
                    <td ><a class="btn btn-primary" id="linkResgatar" href="#" >Verificar</a></td>
                {{else}}

                    {{#if dataSolicitacaoResgate}}
                        <td ><a class="btn btn-primary" id="linkResgatar" href="#" >Alterar</a></td>
                    {{else}}
                        {{#if estahNoPrazoParaResgate}}
                            <td ><a class="btn btn-primary" id="linkResgatar" href="#" >Resgatar</a></td>
                        {{else}}
                            <td ><a class="btn btn-primary" id="linkResgatar" href="#" >Prazo expirado</a></td>
                        {{/if}}
                    {{/if}}


                {{/if}}


                </tr>
                {{/each}}
                </tbody>
                {{/if}}
            </table>
        </div>
    </script>

    <script id="tabelaContatoDetalhe" type="text/x-handlebars-template">
        <div class="row">
            <table class="table table-striped table-hover table-condensed" style="text-align: left">
                <thead>
                <th>TELEFONE</th>
                {{#if listTelefones}}
                <tr>
                    <th>Tipo</th>
                    <th>DDD</th>
                    <th>Número</th>
                </tr>
                </thead>
                <tbody>
                {{#each listTelefones}}
                <tr>
                    <td>
                        {{#compare tipo '1' operator="=="}}Comercial{{/compare}}
                        {{#compare tipo '2' operator="=="}}Residencial{{/compare}}
                        {{#compare tipo '3' operator="=="}}Celular {{/compare}}
                        {{#compare tipo '4' operator="=="}}Contato {{/compare}}
                        {{#compare tipo '5' operator="=="}}Recado {{/compare}}
                        {{#compare tipo '6' operator="=="}}Fax {{/compare}}
                    </td>
                    <td>{{ddd}}</td>
                    <td>{{numero}}</td>
                </tr>
                {{/each}}
                </tbody>
                {{else}}
                <th class="centralizado">Nenhum telefone cadastrado.</th>
                {{/if}}
            </table>
        </div>

        <div class="row">
            <table class="table table-striped table-hover table-condensed" style="text-align: left">
                <thead>
                <th>EMAIL</th>
                {{#if listEmails}}
                <tr>
                    <th>Tipo</th>
                    <th>Descrição</th>
                </tr>
                </thead>
                <tbody>
                {{#each listEmails}}
                <tr>
                    <td>
                        {{#compare tipo '1' operator="=="}}Comercial{{/compare}}
                        {{#compare tipo '2' operator="=="}}Pessoal{{/compare}}
                        {{#compare tipo '3' operator="=="}}Cobrança {{/compare}}
                    </td>
                    <td>{{descricao}}</td>
                </tr>
                {{/each}}
                </tbody>
                {{else}}
                <th class="centralizado">Nenhum email cadastrado.</th>
                {{/if}}
            </table>
        </div>
    </script>

    <script id="tabelaContatoGanhadoresRel" type="text/x-handlebars-template">
        <div class="row">
            <table class="table table-striped table-hover table-condensed" style="text-align: left">
                <thead>
                {{#if listContatos}}
                </thead>
                <tbody>
                {{#each listContatos}}
                <!-- espaço entre os nomes -->
                <td colspan="11">&nbsp;</td>
                <tr>
                    <td ><strong>Nome:</strong> {{nome}}</td>
                    <td><strong>CPF:</strong> {{maskCpf cpf}}</td>
                    <td><strong>Sorteio:</strong> {{sorteio}}</td>
                    <td><strong>Nº Bilhete:</strong> {{numeroBilhete}}</td>
                    <td><strong>Valor:</strong> {{maskMoneyVirgula valor}}</td>
                    {{#if contaBancaria}}
                    <td><strong>Dados Bancários:</strong> Sim</td>
                    {{else}}
                    <td><strong>Dados Bancários:</strong> Não</td>
                    {{/if}}

                    <!-- listagem de email -->
                    <thead>
                    {{#if listEmails}}
                <tr>
                    <th colspan="11">Email</th>
                </tr>
                </thead>
                <tbody>
                {{#each listEmails}}
                <tr>
                    <td colspan="8">
                        {{#compare tipo '1' operator="=="}}Comercial{{/compare}}
                        {{#compare tipo '2' operator="=="}}Pessoal{{/compare}}
                        {{#compare tipo '3' perator="=="}}Cobrança {{/compare}} - {{descricao}}
                    </td>
                </tr>
                </tbody>
                {{/each}}
                {{/if}}

                <!-- listagem de telefones -->
                <thead >
                {{#if listTelefones}}
                <tr>
                    <th>Telefone</th>
                </tr>
                </thead>
                <tbody>
                {{#each listTelefones}}
                <tr>
                    <td colspan="8">
