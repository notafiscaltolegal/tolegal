                    <td >{{maskMoneyVirgula valorTaxasBancarias}}</td>
                    <td >{{nomeBanco}}</td>
                    <td >{{codigoAgencia}}</td>

                    <td >
                        {{#compare tipoConta 1 operator="=="}}Corrente {{/compare}}
