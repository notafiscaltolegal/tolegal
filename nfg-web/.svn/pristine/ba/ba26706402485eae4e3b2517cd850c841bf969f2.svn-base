    <script id="tabelaPremiacoesUsuario" type="text/x-handlebars-template">
        <div class="row">
            <table class="table table-striped table-hover table-condensed" style="text-align: left; margin-bottom: 0;">
                <thead>
                {{#if premiacoes}}
                <tr>
                    <th>Nº</th>
                    <th>CPF</th>
                    <th>Nome</th>
                    <th>Valor Prêmio</th>
                    <th>D.Limite </th>
                    <th>Tipo Resgate</th>
                    <th>Taxas Bancárias</th>
                    <th>Banco</th>
                    <th>Agência</th>
                    <th>Tipo Conta</th>
                    <th>Conta</th>
                    <th>Valor de Tributos</th>
                    <th>Valor Líquido</th>
                    <th>D. Solicitação</th>
                    <th>D. Pagamento</th>
                    <th>Info </th>
                    <th hidden="true">  </th>
                </tr>
                </thead>
                <tbody>
                {{#each premiacoes}}
